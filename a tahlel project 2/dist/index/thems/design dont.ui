    padding: 0px;
    min-width: 70px; /* necessary to show its content */
}

QToolBar &gt; QComboBox,
QToolBar &gt; QAbstractSpinBox,
QToolBar &gt; QSpinBox,
QToolBar &gt; QDoubleSpinBox,
QToolBar &gt; QLineEdit,
QToolBar &gt; QTextEdit,
